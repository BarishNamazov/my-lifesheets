{
	"app": {
		"name": "Sleepy",
		"slug": "sleepy",
		"entry_name": "sleep",
		"entries_name": "sleeps",
		"description": "sleep tracker",
		"private": false,
		"icon": "ðŸ˜´",
		"color": "#520e71",
		"storage_type": "",
		"storage": "",
		"custom_css": "",
		"charts_placement": ""
	},
	"temporal": {
		"times": "",
		"ranges": ""
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				],
				"appearance": "stretch"
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "time",
				"prefix": "fell asleep at ",
				"default": "[time($startup, 'hours')]",
				"appearance": "stretch"
			},
			"name": "fell_asleep"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"prefix": "woke up at ",
				"type": "time",
				"default": "[time(fell_asleep+1000*60*60*8, 'hours')]",
				"appearance": "stretch"
			},
			"name": "woke_up"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "if(woke_up > fell_asleep, duration(woke_up-fell_asleep), 'HMMM')",
				"prefix": "sleep duration ",
				"appearance": ""
			},
			"name": "sleep_duration"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "âž• Add sleep",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [],
				"filter_pairs": []
			}
		}
	],
	"charts": []
}